{
  "name": "spot-react",
  "version": "2.0.0",
  "description": "A sample React.js application that uses Spotify API as backend",
  "scripts": {
    "dev": "webpack-dev-server --hot --inline --devtool eval-source --progress --colors",
    "build": "webpack --config webpack-production.config.js  --progress --profile --colors",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "jsx",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "Emanuele DelBono (@emadb)",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "bootstrap": "^3.3.5",
    "chai": "^3.3.0",
    "css-loader": "^0.19.0",
    "errno": "^0.1.4",
    "eslint": "^1.6.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "flux": "^2.1.1",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "node-sass": "^3.3.3",
    "numeral": "^1.5.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.0-rc3",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "superagent": "^1.4.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
